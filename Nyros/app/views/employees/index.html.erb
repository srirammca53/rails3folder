<h1>Listing Employees</h1>
<%#= link_to "home index", "home/index" %>
<table>
  <tr>
    <th>Employee name</th>
    <th>Employee department</th>
    <th>Date</th>
    <th>Present status</th>
    <th>late</th>
    <th>Absent</th>
   
  </tr>

<% @employees.each do |employee| %>
  <tr>
    <td width="300px"><%= employee.name %></td>
    <td width="300px"><%= employee.date %></td>
    <td width="300px"><%= employee.present %></td>
    <td width="300px"><%= employee.late %></td>
    <td width="300px"><%= employee.absent %></td>
    <td width="300px"><%= link_to 'Show', employee %></td>
    <td width="300px"><%= link_to 'Edit', edit_employee_path(employee) %></td>
    <td width="300px"><%= link_to 'Destroy', employee, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Account', new_employee_path %>
